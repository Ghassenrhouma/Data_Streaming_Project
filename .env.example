# OpenSky API credentials (OAuth2 Client Credentials)
# Get yours at: https://opensky-network.org/
OPENSKY_CLIENT_ID=your_client_id
OPENSKY_CLIENT_SECRET=your_client_secret

# Kafka
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC=opensky-flights

# Java (PySpark 4.x requires JDK 17+)
JAVA_HOME=C:\Program Files\Microsoft\jdk-17.0.18.8-hotspot

# Hadoop (Windows only)
HADOOP_HOME=C:\hadoop
